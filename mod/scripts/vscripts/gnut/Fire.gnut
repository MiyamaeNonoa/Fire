untyped
global function hasNonDigit
global function isdigit
global function RoundFloat


bool function hasNonDigit(str) {
    foreach(ch in str)
    {
        if(ch < '0' || ch > '9')
        {
            return true
        }
    }
    return false
}

bool function isdigit(ch)
{
    return ch >= '0' && ch <= '9'
}

float function RoundFloat(float value, int decimalPlaces)
{
    float factor = pow(10, decimalPlaces)
    return floor(value * factor + 0.5) / factor
}